  synchronite_constants_constants = {
    defaultChannel: '/pl',
    serviceChannel: '/t/sn',
    cssProxySuffix: '/cssproxy',
    imgProxySuffix: '/imgproxy',
    fontProxySuffix: '/fontproxy',
    cobrowsingFrameName: 'displayContent',
    // storage keys
    ticketIdKey: 's.tid',
    cometReloadKey: 's.connect',
